        </activity>
        <activity android:name=".screens.demo.DemoActivity">

        </activity>
        <activity android:name=".screens.createchat.CreatechatActivity" />
        <activity android:name=".screens.main.MainActivity" />
        <activity android:name=".screens.chat.ChatActivity" />
        <activity android:name=".screens.info.InfoActivity" />
